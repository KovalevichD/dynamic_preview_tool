{
  "name": "dynamic_preview_tool",
  "version": "1.0.0",
  "description": "server for dynamic preview tool",
  "main": "app/app.js",
  "scripts": {
    "start": "node app/app.js",
    "server": "nodemon app/app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "nodemonConfig": {
    "ignore": ["app/allVariations/"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KovalevichD/dynamic_preview_tool.git"
  },
  "keywords": [
    "express",
    "server"
  ],
  "author": "Dmitry Kovalevich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KovalevichD/dynamic_preview_tool/issues"
  },
  "homepage": "https://github.com/KovalevichD/dynamic_preview_tool#readme",
  "dependencies": {
    "archiver": "^5.3.0",
    "concurrently": "^6.3.0",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-validator": "^6.13.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "googleapis": "^89.0.0",
    "nodemon": "^2.0.14"
  }
}
